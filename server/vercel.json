{
    "version": 2,
    "name": "tradesphere-backend",

    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/favicon.ico",
        "dest": "/public/favicon.ico"
      },
      {
        "src": "/(.*)", 
        "dest": "/server.js"
      }
    ]
}